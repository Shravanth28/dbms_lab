SELECT loan_number, branch_name, CONCAT(amount/100000,' lakhs') as 'assets in lakhs' FROM loan;

select distinct d.customer_name from depositer d,bankaccount b
where d.accno = b.accno 
group by b.branch_name
HAVING COUNT(d.customer_name) > 2;
